pipeline{
    agent any
      stages{
        stage("Git Checkout"){
            steps{
                  git credentialsId: 'github1', url: 'https://github.com/rehman786muneeb/02---Jenkins-Declarative-github.git'
              }
          }
        stage("Maven Build"){
            steps{
                def mvnHome = tool name: 'Maven3.8.6', type: 'maven'
                def mavenCMD = "${mavenHome}/bin/mvn"
                sh "${mavenCMD} clean package"
              }
          }
      }
}
